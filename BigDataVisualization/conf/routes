# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index
GET     /popularHashtags            controllers.Application.popularHashtags
GET     /numberOfDevices            controllers.Application.numberOfDevices
GET     /userPerTimeZone            controllers.Application.userPerTimeZone
GET     /topUserMentions            controllers.Application.topUserMentions
GET     /sentimentAnalysis          controllers.Application.sentimentAnalysis
GET     /languageCount              controllers.Application.languageCount
GET     /tweetsPerState             controllers.Application.tweetsPerState
GET     /hashtagExplorer            controllers.Application.hashtagExplorer
GET     /textExplorer               controllers.Application.textExplorer
GET     /sentimentsPerTimezone      controllers.Application.sentimentsPerTimezone
GET     /genderPerTimezone          controllers.Application.genderPerTimezone
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
GET     /Visualization/*file        controllers.ExternalAssets.at(path="/Users/rishikaidnani/Desktop/BigData/", file)